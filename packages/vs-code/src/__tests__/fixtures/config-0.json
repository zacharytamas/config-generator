{
  "editor.codeActionsOnSave": { "source": { "fixAll": true } },
  "editor.emptySelectionClipboard": false,
  "editor.fontFamily": "Rec Mono Duotone, BerkeleyMonoVariable-Regular, Cascadia Code PL, MonoLisa, Menlo, Monaco, monospace",
  "editor.lineHeight": 31,
  "editor.fontLigatures": true,
  "editor.fontSize": 15,
  "editor.fontWeight": "500",
  "editor.formatOnSave": true,
  "editor.lineNumbers": "relative",
  "editor.minimap.enabled": false,
  "editor.renderWhitespace": "boundary",
  "editor.rulers": [100],
  "editor.smoothScrolling": true,
  "editor.suggestSelection": "first",
  "editor.tabCompletion": "on",
  "editor.tabSize": 2,
  "editor.wordWrapColumn": 100,
  "eslint.validate": ["javascript", "javascriptreact", "typescript", "typescriptreact"],
  "explorer.confirmDelete": false,
  "explorer.confirmDragAndDrop": false,
  "files.trimTrailingWhitespace": true,
  "git.autofetch": true,
  "git.confirmSync": false,
  "js/ts.implicitProjectConfig.checkJs": true,
  "javascript.updateImportsOnFileMove.enabled": "always",
  "rewrap.reformat": true,
  "rewrap.wrappingColumn": 90,
  "terminal.integrated.cursorBlinking": true,
  "terminal.integrated.fontSize": 15,
  "terminal.integrated.fontWeight": "500",
  "typescript.updateImportsOnFileMove.enabled": "always",
  "vim.normalModeKeyBindingsNonRecursive": [
    { "before": [":"], "commands": ["workbench.action.showCommands"] },
    { "before": ["<leader>", "f"], "commands": ["revealInExplorer"] },
    { "before": ["<leader>", "r", "f"], "commands": ["fileutils.renameFile"] },
    { "before": ["escape"], "commands": ["workbench.action.files.save"] }
  ],
  "vim.insertModeKeyBindingsNonRecursive": [],
  "vim.visualModeKeyBindingsNonRecursive": [
    { "before": [">"], "commands": ["editor.action.indentLines"] },
    { "before": ["<"], "commands": ["editor.action.outdentLines"] }
  ],
  "window.newWindowDimensions": "inherit",
  "vim.enableNeovim": true,
  "workbench.editor.splitSizing": "split",
  "importSorter.generalConfiguration.sortOnBeforeSave": true,
  "importSorter.sortConfiguration.joinImportPaths": true,
  "importSorter.sortConfiguration.removeUnusedImports": true,
  "importSorter.sortConfiguration.removeUnusedDefaultImports": false,
  "importSorter.importStringConfiguration.maximumNumberOfImportExpressionsPerLine.type": "newLineEachExpressionAfterCountLimitExceptIfOnlyOne",
  "importSorter.importStringConfiguration.maximumNumberOfImportExpressionsPerLine.count": 100,
  "importSorter.importStringConfiguration.trailingComma": "multiLine",
  "importSorter.importStringConfiguration.tabSize": 2,
  "importSorter.sortConfiguration.customOrderingRules.rules": [
    { "regex": "^react", "orderLevel": 5 },
    { "type": "importMember", "regex": "^$", "orderLevel": 10 },
    { "regex": "^[@]", "orderLevel": 30 },
    { "regex": "^[.]", "orderLevel": 40 }
  ],
  "workbench.editor.limit.enabled": true,
  "workbench.editor.limit.perEditorGroup": true,
  "workbench.editor.limit.value": 8,
  "editor.semanticHighlighting.enabled": true,
  "workbench.startupEditor": "newUntitledFile",
  "editor.renderControlCharacters": true,
  "beancountFormatter.currencyColumn": 90,
  "beancount.separatorColumn": 90,
  "[beancount]": { "editor.defaultFormatter": "dongfg.vscode-beancount-formatter" },
  "[prisma]": { "editor.defaultFormatter": "Prisma.prisma" },
  "vim.neovimPath": "/opt/homebrew/bin/nvim",
  "markdown.preview.scrollEditorWithPreview": false,
  "remote.SSH.showLoginTerminal": true,
  "debug.javascript.autoAttachFilter": "onlyWithFlag",
  "wallaby.startAutomatically": false,
  "editor.accessibilitySupport": "off",
  "projectManager.git.baseFolders": ["~/Code", "~/Development"],
  "workbench.productIconTheme": "fluent-icons",
  "print.fontSize": "9pt",
  "print.colourScheme": "Atelier Forest",
  "editor.guides.bracketPairs": true,
  "cSpell.userWords": ["baseui", "jscodeshift", "SIGNALFX", "Styletron", "subapps"],
  "diffEditor.renderSideBySide": false,
  "workbench.preferredDarkColorTheme": "Cobalt Next",
  "workbench.editor.enablePreviewFromCodeNavigation": true,
  "workbench.editor.highlightModifiedTabs": true,
  "explorer.experimental.fileNesting.enabled": true,
  "explorer.openEditors.sortOrder": "fullPath",
  "window.title": "${activeEditorShort}${separator}${rootPath}",
  "workbench.colorTheme": "Tokyo Night",
  "workbench.iconTheme": "material-icon-theme",
  "terminal.external.osxExec": "iTerm.app",
  "terminal.integrated.confirmOnExit": "hasChildProcesses",
  "terminal.integrated.cursorStyle": "line",
  "terminal.integrated.cursorWidth": 2,
  "terminal.integrated.defaultLocation": "editor",
  "terminal.integrated.splitCwd": "workspaceRoot"
}
